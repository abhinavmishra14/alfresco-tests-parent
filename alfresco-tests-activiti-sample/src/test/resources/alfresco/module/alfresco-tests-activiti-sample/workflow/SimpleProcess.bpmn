            <activiti:field name="script">
              <activiti:string><![CDATA[if (typeof bpm_workflowDueDate != 'undefined') task.dueDate = bpm_workflowDueDate
                        if (typeof bpm_workflowPriority != 'undefined') task.priority = bpm_workflowPriority;]]></activiti:string>
            </activiti:field>
          </activiti:taskListener>
          <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
            <activiti:field name="script">
              <activiti:string><![CDATA[if (task.getVariableLocal('mccwf_reviewPdA') == 'Continua') {
execution.setVariable('mccwf_resubmit', false);
execution.setVariable('mccwf_cancelPdA', false);
} else if (task.getVariableLocal('mccwf_reviewPdA') == 'Riavvia'){
execution.setVariable('mccwf_resubmit', true);
execution.setVariable('mccwf_cancelPdA', false);
}else if (task.getVariableLocal('mccwf_reviewPdA') == 'Cancella PdA'){
execution.setVariable('mccwf_cancelPdA', true);
execution.setVariable('mccwf_resubmit', false);
}]]></activiti:string>
            </activiti:field>
          </activiti:taskListener>
        </extensionElements>
