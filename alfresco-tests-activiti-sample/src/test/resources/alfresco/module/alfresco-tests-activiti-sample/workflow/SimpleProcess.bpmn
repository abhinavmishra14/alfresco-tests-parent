    <sequenceFlow id="flow39" sourceRef="boundaryerror1" targetRef="subscriptionErrorMailTask"></sequenceFlow>
    <sequenceFlow id="flow40" sourceRef="boundaryerror2" targetRef="labelingErrorMailTask"></sequenceFlow>
